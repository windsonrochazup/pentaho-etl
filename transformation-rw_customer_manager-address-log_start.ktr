  <step>
    <name>log_start_address_rw</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>bi_claro_report</connection>
    <sql>WITH count_created AS (

	SELECT 
		(now() - INTERVAL '3 HOUR')::date AS syncronize_date,
		COUNT(*) as rw_insert_qtde
	FROM bi_claro_flex.address_fk
	WHERE 
	(
		created_at::date = (now() - INTERVAL '3 HOUR')::date 
	)
),

count_updated AS (

	SELECT 
		(now() - INTERVAL '3 HOUR')::date AS syncronize_date,
		COUNT(*) AS rw_update_qtde
	FROM bi_claro_flex.address_fk
	WHERE 
	(
		updated_at::date = (now() - INTERVAL '3 HOUR')::date 
	)
)


SELECT 
	cre.syncronize_date,
	'30 MIN' AS job_type,
	'bi_claro_flex.address_fk'	AS rw_data_base,
	'address' AS rw_table,
	
	cre.rw_insert_qtde,
	up.rw_update_qtde,
	0 AS rw_others_qtde,

	cre.rw_insert_qtde + up.rw_update_qtde AS rw_transactions,

	0 AS diff_insert_qtde,
 	0 AS diff_update_qtde,
	0 AS diff_others_qtde,
	0 AS diff_transactions,

	(now() - INTERVAL '1 HOUR') AS started_at,
	(now() - INTERVAL '1 HOUR') AS ended_at,
	(now() - INTERVAL '1 HOUR') AS update_at,
	'00:00:00' AS syncronization_time,

	'STARTING SYNCHRONIZATION' AS syncronize_status

FROM count_created cre
LEFT JOIN count_updated up ON up.syncronize_date = cre.syncronize_date


</sql>
