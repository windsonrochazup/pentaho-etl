    <connection>bi_claro_report</connection>
    <sql>
WITH origin AS (

	SELECT 
		id,
		id::text 
		|| '_' ||
		CASE WHEN created_at IS NOT NULL THEN created_at::text ELSE '' END
		|| '-' ||
		CASE WHEN updated_at IS NOT NULL THEN updated_at::text ELSE '' END
		|| '_' ||
		CASE WHEN activation_date IS NOT NULL THEN activation_date::text ELSE '' END
		|| '-' ||
		CASE WHEN inactivation_date IS NOT NULL THEN inactivation_date::text ELSE '' END
		|| '_' ||
		CASE WHEN inactivating_date IS NOT NULL THEN inactivating_date::text ELSE '' END
		AS id_ref
			
	FROM bi_claro_flex.product_fk
	WHERE (
			created_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR updated_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR activation_date::date = (now() - INTERVAL '3 HOUR')::date 
			OR inactivation_date::date = (now() - INTERVAL '3 HOUR')::date 
			OR inactivating_date::date = (now() - INTERVAL '3 HOUR')::date
		)

),

destiny AS (

	SELECT 
		id,
		id::text 
		|| '_' ||
		CASE WHEN created_at IS NOT NULL THEN created_at::text ELSE '' END
		|| '-' ||
		CASE WHEN updated_at IS NOT NULL THEN updated_at::text ELSE '' END
		|| '_' ||
		CASE WHEN activation_date IS NOT NULL THEN activation_date::text ELSE '' END
		|| '-' ||
		CASE WHEN inactivation_date IS NOT NULL THEN inactivation_date::text ELSE '' END
		|| '_' ||
		CASE WHEN inactivating_date IS NOT NULL THEN inactivating_date::text ELSE '' END
		AS id_ref
			
	FROM "dash_claro_flex-rw_customer_inventory".product
	WHERE (
			created_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR updated_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR activation_date::date = (now() - INTERVAL '3 HOUR')::date 
			OR inactivation_date::date = (now() - INTERVAL '3 HOUR')::date 
			OR inactivating_date::date = (now() - INTERVAL '3 HOUR')::date
		)

),

origin_destiny AS (

	SELECT o.id 
	FROM origin o
	LEFT JOIN destiny d ON d.id_ref = o.id_ref
	WHERE d.id IS NULL

)

SELECT
	id, customer_id, type_name,
	attributes::text AS attributes,
	external_id, foreign_id, foreign_label, activation_date, created_at, updated_at, status, type_id, inactivation_date, inactivation_reason, inactivated_by, inactivating_date, inactivating_reason, inactivating_by, create_channel
FROM bi_claro_flex.product_fk
WHERE id IN (
	SELECT * FROM origin_destiny
)</sql>
