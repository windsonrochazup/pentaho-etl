    <connection>bi_claro_report</connection>
    <sql>WITH count_created AS (

	SELECT 
		(now() - INTERVAL '3 HOUR')::date AS syncronize_date,
		COUNT(*) as dash_insert_qtde
	FROM "dash_claro_flex-rw_customer_inventory".product
	WHERE 
	(
		created_at::date = (now() - INTERVAL '3 HOUR')::date 
	)
),

count_updated AS (

	SELECT 
		(now() - INTERVAL '3 HOUR')::date AS syncronize_date,
		COUNT(*) AS dash_update_qtde
	FROM "dash_claro_flex-rw_customer_inventory".product
	WHERE 
	(
		updated_at::date = (now() - INTERVAL '3 HOUR')::date 
	)
),

count_others AS (

	SELECT 
		(now() - INTERVAL '3 HOUR')::date AS syncronize_date,
		COUNT(*) AS dash_others_qtde
	FROM "dash_claro_flex-rw_customer_inventory".product
	WHERE 
	(	
		activation_date::date = (now() - INTERVAL '3 HOUR')::date 
		OR inactivation_date::date = (now() - INTERVAL '3 HOUR')::date 
		OR inactivating_date::date = (now() - INTERVAL '3 HOUR')::date
	)

),

logs_dash AS (

	SELECT 
		syncronize_date,
		syncronize_loop,
		rw_table,
		rw_insert_qtde,
		rw_update_qtde,
		rw_others_qtde,
		rw_transactions,
		started_at
	FROM dash_claro_flex.logs_dash
	WHERE syncronize_date::date = (now() - INTERVAL '3 HOUR')::date
		AND rw_table = 'product'
	

)


SELECT 
	cre.syncronize_date,

	CASE
		WHEN logs.syncronize_loop IS NULL THEN 1
		ELSE logs.syncronize_loop + 1
	END
	AS syncronize_loop,

	'dash_claro_flex-rw_customer_inventory.product' AS dash_data_base,
	'product' AS dash_table,
	
	cre.dash_insert_qtde,
	up.dash_update_qtde,
	ot.dash_others_qtde,
	cre.dash_insert_qtde + up.dash_update_qtde + ot.dash_others_qtde AS dash_transactions,

	logs.rw_insert_qtde - cre.dash_insert_qtde AS diff_insert_qtde,
	logs.rw_update_qtde - up.dash_update_qtde AS diff_update_qtde,
	logs.rw_others_qtde - ot.dash_others_qtde AS diff_others_qtde,
	logs.rw_transactions - (cre.dash_insert_qtde + up.dash_update_qtde + ot.dash_others_qtde) AS diff_transactions,


	(now() - INTERVAL '1 HOUR') AS ended_at,
	to_char(AGE(now(), logs.started_at), 'HH24:MI:SS') AS syncronization_time,
	(now() - INTERVAL '1 HOUR') AS update_at,
	
	CASE
		WHEN logs.rw_transactions - (cre.dash_insert_qtde + up.dash_update_qtde + ot.dash_others_qtde) &lt;= 0 THEN 'SYNCHRONIZED'
		WHEN logs.rw_transactions - (cre.dash_insert_qtde + up.dash_update_qtde + ot.dash_others_qtde) &lt; 100 THEN 'SYNCHRONIZING'
		WHEN logs.rw_transactions - (cre.dash_insert_qtde + up.dash_update_qtde + ot.dash_others_qtde) > 100 THEN 'FAIL'
		ELSE 'FAIL'
	END 
	AS syncronize_status



FROM count_created cre
	LEFT JOIN count_updated up ON up.syncronize_date = cre.syncronize_date
	LEFT JOIN count_others ot ON ot.syncronize_date = cre.syncronize_date
	LEFT JOIN logs_dash logs ON logs.syncronize_date = cre.syncronize_date</sql>
