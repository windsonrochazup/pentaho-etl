    <connection>bi_claro_report</connection>
    <sql>
WITH origin AS (

	SELECT 
		id,
		id::text 
		|| '_' ||
		CASE WHEN created_at IS NOT NULL THEN created_at::text ELSE '' END
		|| '-' ||
		CASE WHEN updated_at IS NOT NULL THEN updated_at::text ELSE '' END
		AS id_ref
			
	FROM bi_claro_flex.address_fk
	WHERE (
			created_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR updated_at::date = (now() - INTERVAL '3 HOUR')::date 
		)

),

destiny AS (

	SELECT 
		id,
		id::text 
		|| '_' ||
		CASE WHEN created_at IS NOT NULL THEN created_at::text ELSE '' END
		|| '-' ||
		CASE WHEN updated_at IS NOT NULL THEN updated_at::text ELSE '' END
		AS id_ref
			
	FROM "dash_claro_flex-rw_customer_manager".address
	WHERE (
			created_at::date = (now() - INTERVAL '3 HOUR')::date 
			OR updated_at::date = (now() - INTERVAL '3 HOUR')::date 
		)

),

origin_destiny AS (

	SELECT o.id 
	FROM origin o
	LEFT JOIN destiny d ON d.id_ref = o.id_ref
	WHERE d.id IS NULL

)

SELECT 
	id, customer_id, street, "number", zipcode, district, city, state, country, name, complement, created_at, updated_at, status,
	custom_fields::text AS custom_fields, 
	description, address, address_type, postal_code
FROM bi_claro_flex.address_fk
WHERE id IN (
	SELECT * FROM origin_destiny
)</sql>
